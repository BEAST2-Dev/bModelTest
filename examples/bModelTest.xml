<?xml version="1.0" encoding="UTF-8" standalone="no"?><beast beautitemplate='Standard' beautistatus='' namespace="beast.core:beast.evolution.alignment:beast.evolution.tree.coalescent:beast.core.util:beast.evolution.nuc:beast.evolution.operators:beast.evolution.sitemodel:beast.evolution.substitutionmodel:beast.evolution.likelihood" version="2.0">


    <data
id="dna"
name="alignment">
                    <sequence id="seq_Cow" taxon="Cow" totalcount="4" value="---------------------TAA"/>
                    <sequence id="seq_Carp" taxon="Carp" totalcount="4" value=""/>
                    <sequence id="seq_Chicken" taxon="Chicken" totalcount="4" value="ATGGCCAACCACTCCCAACTAGGCTTTCAAGACGCCTCATCCCCCATCATAGAAGAGCTCGTTGAATTCCACGACCACGCCCTGATAGTCGCACTAGCAATTTGCAGCTTAGTACTCTACCTTCTAACTCTTATACTTATAGAAAAACTATCA---------------------CTGTCATCTTAA"/>
                    <sequence id="seq_Human" taxon="Human" totalcount="4" value="---------------------GGGCCCGTATTTACCCTATAG"/>
                    <sequence id="seq_Loach" taxon="Loach" totalcount="4" value=""/>
                    <sequence id="seq_Mouse" taxon="Mouse" totalcount="4" value="---------------------TAA"/>
                    <sequence id="seq_Rat" taxon="Rat" totalcount="4" value="---------------------TAA"/>
                    <sequence id="seq_Seal" taxon="Seal" totalcount="4" value="---------------------TAA"/>
                    <sequence id="seq_Whale" taxon="Whale" totalcount="4" value="---------------------TAA"/>
                    <sequence id="seq_Frog" taxon="Frog" totalcount="4" value="---GAAGCATCACTA------AGA"/>
                </data>


    


    


    
<map name="Uniform" >beast.math.distributions.Uniform</map>
<map name="Exponential" >beast.math.distributions.Exponential</map>
<map name="LogNormal" >beast.math.distributions.LogNormalDistributionModel</map>
<map name="Normal" >beast.math.distributions.Normal</map>
<map name="Beta" >beast.math.distributions.Beta</map>
<map name="Gamma" >beast.math.distributions.Gamma</map>
<map name="LaplaceDistribution" >beast.math.distributions.LaplaceDistribution</map>
<map name="prior" >beast.math.distributions.Prior</map>
<map name="InverseGamma" >beast.math.distributions.InverseGamma</map>
<map name="OneOnX" >beast.math.distributions.OneOnX</map>


<run id="mcmc" spec="MCMC" chainLength="10000000">
    <state id="state" storeEvery="5000">
        <tree id="Tree.t:dna" name="stateNode">
            <taxonset id="TaxonSet.dna" spec="TaxonSet">
                <alignment idref="dna"/>
            </taxonset>
        </tree>
        <parameter id="birthRate.t:dna" name="stateNode">1.0</parameter>
        <stateNode id="BMTmodelIndicator.s:dna" spec="parameter.IntegerParameter" lower="0">0</stateNode>
        <parameter id="BMTrates.s:dna" dimension="6" lower="0.0" name="stateNode" upper="6.0">1.0</parameter>
        <parameter id="gammaShape.s:dna" name="stateNode">1.0</parameter>
        <parameter id="proportionInvariant.s:dna" lower="0.0" name="stateNode" upper="1.0">0.1</parameter>
        
        <stateNode id="hasInvariantSites.s:dna" spec="parameter.IntegerParameter">1</stateNode>        
        <stateNode id="hasGammaRates.s:dna" spec="parameter.IntegerParameter">1</stateNode>     
        <!-- frequencies estimation -->
        <stateNode id="hasEqualFreqs.s:dna" spec="parameter.BooleanParameter">false</stateNode>
        <stateNode id="frequencies.s:dna" spec="parameter.RealParameter">0.25 0.25 0.25 0.25</stateNode>
    </state>

    <init id="RandomTree.t:dna" spec="beast.evolution.tree.RandomTree" estimate="false" initial="@Tree.t:dna" taxa="@dna">
        <populationModel id="ConstantPopulation0.t:dna" spec="ConstantPopulation">
            <parameter id="randomPopSize.t:dna" name="popSize">1.0</parameter>
        </populationModel>
    </init>

    <distribution id="posterior" spec="util.CompoundDistribution">
        <distribution id="prior" spec="util.CompoundDistribution">
            <distribution id="YuleModel.t:dna" spec="beast.evolution.speciation.YuleModel" birthDiffRate="@birthRate.t:dna" tree="@Tree.t:dna"/>

            <distribution id="BMTprior.s:dna" spec="beast.math.distributions.NucleotideRevJumpSubstModelRatePrior"
                priorType="onTransitionsAndTraversals"
                modelIndicator="@BMTmodelIndicator.s:dna" x="@BMTrates.s:dna" substModel="@RevJump.s:dna"/>
            <prior id="YuleBirthRatePrior.t:dna" name="distribution" x="@birthRate.t:dna">
                <Uniform id="Uniform.0" name="distr" upper="Infinity"/>
            </prior>

            <distribution id="BMTPropInvariantPrior.s:dna" spec="beast.math.distributions.BMTPrior" x="@proportionInvariant.s:dna" count='@hasInvariantSites.s:dna'>
                <Uniform id="Uniform.01" name="distr" lower="0" upper="1.0"/>
            </distribution>

            <distribution id="BMTGammaShapePrior.s:dna" spec="beast.math.distributions.BMTPrior" x="@gammaShape.s:dna" count='@hasGammaRates.s:dna'>
                <Exponential id="Exponential.0" name="distr">
                    <parameter id="RealParameter.02" lower="0.0" name="mean" upper="0.0">1.0</parameter>
                </Exponential>
            </distribution>
            
            <!-- frequencies estimation -->
            <prior  id="freqsPrior.s:dna"  name="distribution" x="@frequencies.s:dna">
            	<distr spec="beast.math.distributions.Dirichlet" alpha="1.0 1.0 1.0 1.0"/>
            </prior>

        </distribution>
        <distribution id="likelihood" spec="util.CompoundDistribution">
            <distribution id="treeLikelihood.dna" spec="TreeLikelihood" data="@dna" tree="@Tree.t:dna">
                <siteModel id="SiteModel.s:dna" spec="beast.evolution.sitemodel.BEASTModelTest" 
                	gammaCategoryCount="4" proportionInvariant="@proportionInvariant.s:dna" shape="@gammaShape.s:dna"
                	hasGammaRates="@hasGammaRates.s:dna" hasInvariantSites="@hasInvariantSites.s:dna">
                    <parameter id="mutationRate.s:dna" estimate="false" name="mutationRate">1.0</parameter>
                    <substModel id="RevJump.s:dna" spec="beast.evolution.substitutionmodel.NucleotideRevJumpSubstModel" 
                        modelIndicator="@BMTmodelIndicator.s:dna" rates="@BMTrates.s:dna"
                        modelSet="transitionTransversionSplit">
                        
                        <!-- frequencies estimation -->
                        <!-- case where we switch between empirical and equal frequencies
                        <frequencies id="freqs.s:dna" spec="ModelFrequencies" data="@dna" hasEqualFreqs='@hasEqualFreqs.s:dna' emprical="true"/>
                        -->
                        <!-- case where we switch between empirical and estimated frequencies -->
                        <frequencies id="freqs.s:dna" spec="ModelFrequencies" frequencies="@frequencies.s:dna" hasEqualFreqs='@hasEqualFreqs.s:dna' empirical="false"/>
                        
                    </substModel>
                </siteModel>
                <branchRateModel id="StrictClock.c:dna" spec="beast.evolution.branchratemodel.StrictClockModel">
                    <parameter id="clockRate.c:dna" estimate="false" name="clock.rate">1.0</parameter>
                </branchRateModel>
            </distribution>
        </distribution>
    </distribution>

    <operator id="YuleBirthRateScaler.t:dna" spec="ScaleOperator" parameter="@birthRate.t:dna" scaleFactor="0.75" weight="3.0"/>

    <operator id="treeScaler.t:dna" spec="ScaleOperator" scaleFactor="0.5" tree="@Tree.t:dna" weight="3.0"/>

    <operator id="treeRootScaler.t:dna" spec="ScaleOperator" rootOnly="true" scaleFactor="0.5" tree="@Tree.t:dna" weight="3.0"/>

    <operator id="UniformOperator.t:dna" spec="Uniform" tree="@Tree.t:dna" weight="30.0"/>

    <operator id="SubtreeSlide.t:dna" spec="SubtreeSlide" tree="@Tree.t:dna" weight="15.0"/>

    <operator id="narrow.t:dna" spec="Exchange" tree="@Tree.t:dna" weight="15.0"/>

    <operator id="wide.t:dna" spec="Exchange" isNarrow="false" tree="@Tree.t:dna" weight="3.0"/>

    <operator id="WilsonBalding.t:dna" spec="WilsonBalding" tree="@Tree.t:dna" weight="3.0"/>

    <operator id="BMTOperator.s:dna" spec="BMTMergeSplitOperator" modelIndicator="@BMTmodelIndicator.s:dna" rates="@BMTrates.s:dna" substModel="@RevJump.s:dna" weight="1.0"/>
    <operator id="BMTexchanger.s:dna" spec="BMTExchangeOperator" modelIndicator="@BMTmodelIndicator.s:dna" rates="@BMTrates.s:dna" substModel="@RevJump.s:dna"  delta="0.1" weight="1.0"/>

    <operator id="BMTshapeOperator.s:dna" spec="BMTBirthDeathOperator" count="@hasGammaRates.s:dna" rates="@gammaShape.s:dna" weight="1.0"/>
    <operator id="BMTshapecaler.s:dna" spec="BMTScaleOperator" count="@hasGammaRates.s:dna" parameter="@gammaShape.s:dna" scaleFactor="0.5" weight="1.0"/>

    <operator id="BMTpinvOperator.s:dna" spec="BMTBirthDeathOperator" count="@hasInvariantSites.s:dna" rates="@proportionInvariant.s:dna" weight="1.0"/>
    <operator id="BMTpInvscaler.s:dna" spec="BMTScaleOperator" count="@hasInvariantSites.s:dna" parameter="@proportionInvariant.s:dna" scaleFactor="0.5" weight="1.0"/>
   
    <!-- frequencies estimation -->
    <operator id="BMTFreqsFlipOperator.s:dna" spec="BitFlipOperator" parameter="@hasEqualFreqs.s:dna" weight="0.1"/>
    <operator id='FrequenciesExchanger.s:dna' spec='DeltaExchangeOperator' delta="0.01" weight="0.1" parameter="@frequencies.s:dna"/>
    
    
    <logger id="tracelog" fileName="dna.log" logEvery="1000" model="@posterior" sanitiseHeaders="true" sort="smart">
        <log idref="posterior"/>
        <log idref="likelihood"/>
        <log idref="prior"/>
        <log idref="treeLikelihood.dna"/>
        <log id="TreeHeight.t:dna" spec="beast.evolution.tree.TreeHeightLogger" tree="@Tree.t:dna"/>
        <log idref="YuleModel.t:dna"/>
        <log idref="birthRate.t:dna"/>
        <log idref="BMTmodelIndicator.s:dna"/>
		<log idref="RevJump.s:dna"/>
        <log idref="BMTrates.s:dna"/>
        <log idref="gammaShape.s:dna"/>
        
        <log id="ActivePropInvariant" spec='beast.math.statistic.RPNcalculator' expression="proportionInvariant.s:dna hasInvariantSites.s:dna *">
	        <parameter idref="proportionInvariant.s:dna"/>
	        <parameter idref="hasInvariantSites.s:dna"/>
        </log>
        <log id="ActiveGammaShape" spec='beast.math.statistic.RPNcalculator' expression="gammaShape.s:dna hasGammaRates.s:dna *">
	        <parameter idref="gammaShape.s:dna"/>
	        <parameter idref="hasGammaRates.s:dna"/>
        </log>
        <log idref="proportionInvariant.s:dna"/>
        
        <!-- frequencies estimation -->
    	<log idref="hasEqualFreqs.s:dna"/>
    	<log idref="frequencies.s:dna"/>

        <log idref='hasGammaRates.s:dna'/>
        <log idref='hasInvariantSites.s:dna'/>
        
    	<log idref="BMTGammaShapePrior.s:dna"/>
    	<log idref="BMTPropInvariantPrior.s:dna" />
    	
    </logger>

    <logger id="screenlog" logEvery="1000">
        <log idref="posterior"/>
        <log id="ESS.0" spec="util.ESS" arg="@posterior"/>
        <log idref="likelihood"/>
        <log idref="prior"/>
    </logger>

    <logger id="treelog.t:dna" fileName="dna.trees" logEvery="1000" mode="tree">
        <log id="TreeWithMetaDataLogger.t:dna" spec="beast.evolution.tree.TreeWithMetaDataLogger" tree="@Tree.t:dna"/>
    </logger>

</run>

</beast>
